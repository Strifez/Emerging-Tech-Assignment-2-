<% include ../partials/header.ejs %>

  <!-- MAIN CONTENT -->
  <div id="carousel-Outdoors" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#carousel-Outdoors" data-slide-to="0" class="active"></li>
      <li data-target="#carousel-Outdoors" data-slide-to="1"></li>
      <li data-target="#carousel-Outdoors" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
      <div class="item active">
        <img src="Assets/images/fish-photo.JPG" alt="fish at MBG">
        <div class="carousel-caption">
          <h1 class="spfont"> Fishing! </h1>
          <p class="spfontpara"> Fishing can help you to relax, improve focus and builds character! </p>
          <p class="spfontparasmall">This was taken at Montreal Botanical Garden</p>
        </div>
      </div>
      <div class="item">
        <img src="Assets/images/Iris.JPG" alt="Subaru WRX">
        <div class="carousel-caption">
          <h1 class="spfont"> Got to Go Fast! </h1>
          <p class="spfontpara">
            Love my WRX
          </p>
          <p class="spfontparasmall"> This was taken near the highway in Markham</p> 
        </div>
        </div>
      <div class="item">
          <img src="Assets/images/duck-fly.JPG" alt="duck flying">
          <div class="carousel-caption">
            <h1 class="spfont"> Photography </h1>
            <p class="spfontpara"> Photos are more than 1000 words </p>
            <p class="spfontparasmall"> This was taken at a park at my friends wedding</p>
          </div>
      </div>
        Outdoors Carousel Bootstrap CopyRight Jason Huang 2017
      </div>

      <!-- Controls -->
      <a class="left carousel-control" href="#carousel-Outdoors" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#carousel-Outdoors" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>

    <% include ../partials/footer.ejs %>